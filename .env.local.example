# OnchainKit API Key
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key

# Contract Addresses (update after deployment)
NEXT_PUBLIC_CREDENTIAL_SBT_ADDRESS=0x...
NEXT_PUBLIC_PRESCRIPTION_REGISTRY_ADDRESS=0x...

# Pinata IPFS
NEXT_PUBLIC_PINATA_API_KEY=your_pinata_api_key
NEXT_PUBLIC_PINATA_SECRET_KEY=your_pinata_secret_key

# Paymaster Service (for sponsored gas transactions)
# Get this from Coinbase Developer Platform: https://www.coinbase.com/developer-platform
# Navigate to: Onchain Tools > Paymaster
# Note: Use a proxy URL to protect the paymaster service URL in production
NEXT_PUBLIC_PAYMASTER_ENDPOINT_URL=https://api.developer.coinbase.com/rpc/v1/base-sepolia/YOUR_API_KEY

# Project Name
NEXT_PUBLIC_PROJECT_NAME=MedChain

RESEND_API_KEY=re_your_resend_api_key_here
EMAIL_FROM=prescriptions@medchain.app
